{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-implicit-dependencies": [true, "dev"],
    "interface-name": [true, "never-prefix"],
    "object-literal-sort-keys": false,
    "interface-over-type-literal": false
  },
  "rulesDirectory": []
}
